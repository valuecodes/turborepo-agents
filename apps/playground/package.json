{
  "name": "playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3001",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --write .",
    "clean": "rm -rf .turbo node_modules .cache",
    "test": "vitest run"
  },
  "dependencies": {
    "@repo/ui": "workspace:*",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.17",
    "@repo/eslint": "workspace:*",
    "@repo/prettier": "workspace:*",
    "@repo/typescript": "workspace:*",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "eslint": "9.39.1",
    "globals": "16.5.0",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "vite": "7.2.4",
    "vitest": "4.0.16"
  }
}
